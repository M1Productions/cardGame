class Estetics
{
  color getBasicPrimaryColor()
  {
    if(team.equals("cake"))
    { return #DAC6BF; }
    else if(team.equals("music"))
    { return #57897E; }
    else
    { return #845B55; }
  }
  color getBasicSecondaryColor()
  {
    if(team.equals("cake"))
    { return #E59890; }
    if(team.equals("music"))
    { return #769D88; }
    else
    { return #F8EEE5; }
  }
}
